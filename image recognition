import cv2
from ultralytics import YOLO

# Load pre-trained YOLOv8 model (can also use 'yolov8m.pt' or 'yolov8l.pt')
model = YOLO("yolov8n.pt")  # 'n' = nano, fastest and lightest version

# Load an image
image_path = "test.jpg"
img = cv2.imread(image_path)

# Run object detection
results = model(img)
